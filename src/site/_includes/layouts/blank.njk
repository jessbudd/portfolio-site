<!doctype html>
<html lang="en-au">
  <head>
  <!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=UA-103059618-2"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'UA-103059618-2');
</script>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/styles.css">
    <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon3.png">
    <title>{{ title }}</title>
    {%- if meta %}<meta name="description" content="{{ meta | safe }}">{% endif %}
    <!--twitter sharing-->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:creator" content="@jessbudd4">
    <meta name="og:title" content="{{ title | safe }}">
    <meta name="twitter:site" content="@jessbudd4" />
    {%- if meta %}
    <meta name="og:description" content="{{ meta | safe }}">
    {% endif %}
    {%- if img %}
    <meta name="og:image" content="{{ img | safe }}">
    {% endif %}

  </head>
  <body {% if bodyClass %}class="{{ bodyClass }}"{% endif %}>

    <div class="container">


      {{ content | safe }}
      
      {% include "footer-alt.njk" %}


    </div>

    {% set js %}
      {# {% include "js/core.js" %} #}
    {% endset %}


  </body>
</html>



